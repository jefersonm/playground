package com.github.jefersonm.sandbox.scala

object Bingo {

  def montaCartela(list: List) = {
    val cartela = List("1")
  }
  
  def test(num:Int):String = {
    "DEF"
  }
  
  def calc(val1:Integer, val2:Integer, arg:Any): Any = arg match {
    case "+" => val1 + val2
    case "-" => val1 - val2
    case "*" => val1 * val2
    case "/" => val1 / val2
    case  _  => "This operation isn't supported"
  }
  
}